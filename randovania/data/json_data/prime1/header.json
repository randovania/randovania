        "tricks": [
            {
                "index": 0,
                "long_name": "Combat/Scan Dash",
                "short_name": "Dash",
                "description": "By locking onto an enemy or scan point and strafing left or right, Samus' momentum can be maintainted. This is useful in crossing gaps.<br /><a href='https://wiki.metroidprime.run/wiki/Dash_Jump_(Prime)'>Check the wiki for more details.</a>"
            },
            {
                "index": 1,
                "long_name": "Knowledge",
                "short_name": "Knowledge",
                "description": "This trick covers breakable objects that one wouldn't expect to be broken with certain items."
            },
            {
                "index": 2,
                "long_name": "Combat",
                "short_name": "Combat",
                "description": "Mainly used for bosses, requiring either fewer items or less health to beat a boss."
            },
            {
                "index": 3,
                "long_name": "L-Jump",
                "short_name": "LJump",
                "description": "This trick allows Samus to jump farther than usual by holding L during the beginning of a jump."
            },
            {
                "index": 4,
                "long_name": "Invisible Objects",
                "short_name": "InvisibleObjects",
                "description": "Objects that normally need visors to be seen or accessed without the required visor, such as Power Conduits without Thermal Visor or Invisible Platforms without X-Ray Visor."
            },
            {
                "index": 5,
                "long_name": "Damage Boosting",
                "short_name": "DBoosting",
                "description": "Some damage sources give Samus extra velocity, which can be used to give her a small boost."
            },
            {
                "index": 6,
                "long_name": "Movement",
                "short_name": "Movement",
                "description": "A broad category for non-obvious movement which can't easily be classified using other tricks."
            },
            {
                "index": 7,
                "long_name": "Slope Jump",
                "short_name": "SJump",
                "description": "A technique where Samus can gain extra height by L-Jumping into certain slopes."
            },
            {
                "index": 8,
                "long_name": "Standable Terrain",
                "short_name": "Standable",
                "description": "Samus can maneuver in unexpected ways by standing on small ledges, vines, railings, and other unlikely objects."
            },
            {
                "index": 9,
                "long_name": "Bomb Jump",
                "short_name": "BJ",
                "description": "By using the vanilla Bomb Jump mechanic in unexpected places, Samus can clear jumps normally meant to require the use of other items."
            },
            {
                "index": 10,
                "long_name": "Jump Off Enemies",
                "short_name": "StandEnemies",
                "description": "Samus can stand on some non-obvious enemies and jump off of them for extra height."
            },
            {
                "index": 11,
                "long_name": "Clip Through Objects",
                "short_name": "ClipThruObjects",
                "description": "This trick expects Samus to be able to clip through certain objects from the back with Boost Ball or a reposition."
            },
            {
                "index": 12,
                "long_name": "R-Jump",
                "short_name": "RJump",
                "description": "This trick allows Samus to jump farther than usual by pressing R in the air.<br /><a href='https://wiki.metroidprime.run/wiki/R-Jumping'>Check the wiki for more details.</a>"
            },
            {
                "index": 13,
                "long_name": "Bomb Space Jump",
                "short_name": "BSJ",
                "description": "Bomb Jumping and unmorphing forwards from an overhang to get an instant unmorph, then jumping within 2/5 of a second after leaving the ground gives a height boost to reach higher ledges.<br /><a href='https://wiki.metroidprime.run/wiki/Bomb_Space_Jump'>Check the wiki for more details.</a>"
            },
            {
                "index": 14,
                "long_name": "Wall Boost",
                "short_name": "WallBoost",
                "description": "Using Boost Ball, a well timed boost can be used to climb a wall."
            },
            {
                "index": 15,
                "long_name": "Spinners without Boost",
                "short_name": "BoostlessSpiner",
                "description": "Some spinners can be turned without boost ball. Exact method varies by spinner."
            },
            {
                "index": 16,
                "long_name": "Single Room Out of Bounds",
                "short_name": "OoB",
                "description": "Traveling outside the room boundaries to gain access to areas/rooms otherwise unreachable given the circumstances. Currently only used for single-room traversal.<br /><a href='https://wiki.metroidprime.run/wiki/Out_of_Bounds'>Check the wiki for more details.</a>"
            },
            {
                "index": 17,
                "long_name": "Varialess Magmoor",
                "short_name": "VMR",
                "description": "This trick accounts for traverseing across Magmoor without Heat Protection."
            },
            {
                "index": 18,
                "long_name": "Complex Bomb Jump",
                "short_name": "CBJ",
                "description": "By abusing the Bomb Refill timer, Samus can reach heights that are normally inaccessible with a standard bomb jump. This trick accounts for High Bomb Jumps, Half Pipe Bomb Jumps, and Uber Bomb Jumps.<br /><a href='https://wiki.metroidprime.run/wiki/Bomb_Jump'>Check the wiki for more details.</a>"
            },
            {
                "index": 19,
                "long_name": "Complex Movement",
                "short_name": "CMovement",
                "description": "This trick expects precise movement to pull of complicated tricks. Some examples include Geothermal Core without Boost Ball and Great Tree Hall without Spider Ball and Space Jump."
            }
        ],
