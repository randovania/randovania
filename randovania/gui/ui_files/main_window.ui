              <widget class="QWidget" name="echoes_hints_tab">
               <attribute name="title">
                <string>Hints</string>
               </attribute>
               <layout class="QVBoxLayout" name="echoes_hints_tab_layout">
                <property name="spacing">
                 <number>0</number>
                </property>
                <property name="leftMargin">
