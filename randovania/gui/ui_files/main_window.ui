                <item>
                 <widget class="QScrollArea" name="echoes_differences_scroll_area">
                  <property name="widgetResizable">
                   <bool>true</bool>
                  </property>
                  <widget class="QWidget" name="echoes_differences_scroll_contents">
                   <property name="geometry">
                    <rect>
                     <x>0</x>
